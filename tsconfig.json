{
  "compilerOptions": {
    "outDir": "dist",
    "declaration": false,
    "strictNullChecks": true,
    "sourceMap": true,
    "target": "es2017",
    "module": "commonjs",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "skipLibCheck": true,
    "noImplicitAny": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
  }
}